<p id="notice"><%= notice %></p>

<h1>Cart Items</h1>

<table>
  <thead>
    <tr>
      <th>id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% cart_items.each do |cart_item| %>
      <tr>
        <td><%= cart_item["id"] %></td>
        <td><%= link_to 'Destroy', cart_items_destroy_path, product_id: cart_item["id"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
